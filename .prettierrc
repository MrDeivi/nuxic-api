{
	"singleQuote": true,
	"endOfLine": "auto",
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": false,
	"semi": false,
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"trailingComma": "es5",
	"arrowParens": "always",
	"insertPragma": false,
	"requirePragma": false,
	"proseWrap": "never",
	"rangeStart": 0
} 
